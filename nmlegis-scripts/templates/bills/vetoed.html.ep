% layout 'default';
% title  'Vetoed Bills';

% if (! @$bills) {
<h1>No bills have been vetoed</h1>
%     return $_O;
% }
<h1>Vetoed Bills</h1>

<table class="bills">
% for my $b (@$bills) {
%= include 'bills/_billrow', b => $b, columns => [ 'watch_button', 'id', 'title', 'sponsors', 'status' ]
% }
</table>
